{
  "prgname" : "simple",

  "cfg" : {
    "cmdport" : 10000,
    "allowquit" : true,

    "crashhandler" : {
      "enable" : true,
      "file" : "/tmp/callstack.txt"
    },
    "daemonize" : {
      "enable" : false,
      "opt" : "d"
    },
    "foreground" : {
      "enable" : false,
      "opt" : "f"
    },
    "healthcheck" : {
      "enable" : true
    },
    "log" : {
      "lvl" : 255,
      "opt" : "l"
    },
    "memtrack" : {
      "enable" : true,
      "size" : 10000
    },
    "syslog" : {
      "size" : 10000
    },
    "webgui" : {
      "enable" : true,
      "css" : true
    }
  }
}

